GMAIL_PASS=
GOOGLE_EMAIL=
GOOGLE_PASSWORD=

# Google OAuth
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# App/Server
PORT=3000
DOMAIN=localhost
NODE_ENV=development
